{
    "handler":[
        {
            "sink": "app/logs/config_log.log",
            "format": "{level:<6} | {time:YYYY-MM-DD} | {name:<20} | {line} | {message}",
            "rotation": "1 MB",
            "compression": "zip",
            "backtrace": false,
            "diagnose": false,
            "level": "INFO",
            "filter": "config"
        },
        {
            "sink": "app/logs/func_log.log",
            "format": "{level:<6} | {time:YYYY-MM-DD} | {name:<20} | {line} | {message}",
            "rotation": "1 MB",
            "compression": "zip",
            "backtrace": false,
            "diagnose": false,
            "level": "INFO",
            "filter": "func"
        }
    ]
}